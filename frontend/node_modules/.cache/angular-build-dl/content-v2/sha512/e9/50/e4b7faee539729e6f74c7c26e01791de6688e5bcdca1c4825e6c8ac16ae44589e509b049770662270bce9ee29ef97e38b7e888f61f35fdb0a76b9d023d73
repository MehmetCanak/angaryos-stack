    let LoginComponent = class LoginComponent {
      constructor(messageHelper, sessionHelper, generalHelper, aeroThemeHelper, router) {
        this.messageHelper = messageHelper;
        this.sessionHelper = sessionHelper;
        this.generalHelper = generalHelper;
        this.aeroThemeHelper = aeroThemeHelper;
        this.router = router;
        this.loading = false;
        this.user = {
          "email": "iletisim@omersavas.com",
          "password": "1234Aa."
        };
        if (_helpers_base__WEBPACK_IMPORTED_MODULE_3__["BaseHelper"].token.length > 0) window.location.href = _helpers_base__WEBPACK_IMPORTED_MODULE_3__["BaseHelper"].dashboardUrl;
        this.aeroThemeHelper.removeThemeClass();
        this.aeroThemeHelper.pageRutine();
      }

      validate() {
        if (this.user.password.length < 4) {
          this.messageHelper.toastMessage("Şifre en az 4 karakter olmalı", "warning");
          return false;
        } else if (this.user.email.length < 4) {
          this.messageHelper.toastMessage("Mail en az 4 karakter olmalı", "warning");
          return false;
        }

        return true;
      }

      doLogin() {
        if (!this.validate()) return;
        this.loading = true;
        this.sessionHelper.login(this.user.email, this.user.password).then(data => {
          _helpers_base__WEBPACK_IMPORTED_MODULE_3__["BaseHelper"].setToken(data["token"]);

          this.sessionHelper.fillLoggedInUserInfo().then(data => {
            this.loading = false;
            this.loadScript();
            window.location.href = _helpers_base__WEBPACK_IMPORTED_MODULE_3__["BaseHelper"].dashboardUrl;
          }).catch(e => {
            this.loading = false;
          });
        }).catch(errorMessage => {
          this.loading = false;
          if (errorMessage == "***") return;
          this.messageHelper.toastMessage("Doğrulama Hatası: " + errorMessage);
        });
      }

      loadScript() {
        _helpers_base__WEBPACK_IMPORTED_MODULE_3__["BaseHelper"].writeToPipe('loadPageScriptsLoaded', false);

        setTimeout(() => {
          _helpers_base__WEBPACK_IMPORTED_MODULE_3__["BaseHelper"].writeToPipe('loadPageScriptsLightLoaded', false);

          this.aeroThemeHelper.loadPageScriptsLight();
        }, 500);
      }

    };

