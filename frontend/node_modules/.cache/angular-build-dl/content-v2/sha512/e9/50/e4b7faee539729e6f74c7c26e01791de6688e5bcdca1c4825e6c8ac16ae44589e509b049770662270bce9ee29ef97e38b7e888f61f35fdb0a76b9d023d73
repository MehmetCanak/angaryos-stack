      autoFillNameColumnFromDisplayNameColumn(tableName, columnName, elementId, data) {
        var group = this.getFormGroupElement(elementId);
        var params = {
          elementId: elementId,
          columnName: columnName,
          data: data,
          this: this
        };

        function func(params) {
          var targetElementId = params.elementId.replace(params.columnName, 'name');
          var val = $(targetElementId).val();
          if (typeof val != "undefined" && val != "") return;
          var name = params.this.sessionHelper.toSeo(params.data.display_name);
          $(targetElementId).val(name); //return {danger: "test error"+params.elementId, success: "test success"};
        }

        return _base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].doInterval('autoFillNameColumnFromDisplayNameColumn', func, params);
      }

    };

