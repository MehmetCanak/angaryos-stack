        this.aeroThemeHelper.pageRutine();
      }

      keyEvent(event) {
        var keys = ['altKey', 'ctrlKey', 'shiftKey'];

        for (var i = 0; i < keys.length; i++) _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"]['pipe'][keys[i]] = event[keys[i]];
      }

      ngAfterViewInit() {
        if (_helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].readFromPipe('loadPageScriptsLightLoaded')) this.firstPage = false;
        this.setDropdownOpenEffect();
        this.aeroThemeHelper.loadPageScripts();
      }

      setDropdownOpenEffect() {
        var th = this;
        $('.dropdown').on('show.bs.dropdown', function (e) {
          if (th.firstPage) $(this).find('.dropdown-menu').first().stop(true, true).slideUp(300);else $(this).find('.dropdown-menu').first().stop(true, true).slideDown(300);
        });
        $('.dropdown').on('hide.bs.dropdown', function (e) {
          if (th.firstPage) $(this).find('.dropdown-menu').first().stop(true, true).slideDown(300);else $(this).find('.dropdown-menu').first().stop(true, true).slideUp(300);
        });
      }

      getTablesGroups() {
        var rt = [];
        var temp = _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].loggedInUserInfo['menu']['tableGroups'];

        for (var i = 0; i < temp.length; i++) if (temp[i]['table_ids'].length > 0) rt.push(temp[i]);

        return rt;
      }

      getTableName(tableGroupId, tableId) {
        var tables = _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].loggedInUserInfo['menu']['tables'][tableGroupId];

        for (var i = 0; i < tables.length; i++) if (tables[i]['id'] == tableId) return tables[i]['display_name'];
      }

      getTableUrl(tableGroupId, tableId) {
        var tables = _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].loggedInUserInfo['menu']['tables'][tableGroupId];

        for (var i = 0; i < tables.length; i++) if (tables[i]['id'] == tableId) return _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].baseUrl + "table/" + tables[i]['name'];
      }

      openDropDown(i) {
        setTimeout(() => {
          $('.dropdown [data-toggle="dropdown"]:eq(' + i + ')').click();
        }, 100);
      }

      getTableGroupImageUrl(tableGroup) {
        var url = _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].backendBaseUrl;
        if (tableGroup.image) url += tableGroup.image;else url += 'uploads/2020/01/01/nomenuimage.png';
        return url;
      }

      goToPage(page) {
        this.generalHelper.navigate(page);
      }

      getDashboardImageUrl() {
        return _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].backendBaseUrl + "uploads/2020/01/01/dashboard.png";
      }

      linkClicked() {
        _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].writeToPipe('loadPageScriptsLoaded', false);

