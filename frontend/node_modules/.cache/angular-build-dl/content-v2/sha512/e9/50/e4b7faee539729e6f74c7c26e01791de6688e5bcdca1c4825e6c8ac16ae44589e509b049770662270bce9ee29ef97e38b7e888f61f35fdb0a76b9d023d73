      onChanged(event) {
        var temp = window.location.href;
        temp = temp.replace(_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].baseUrl, "");
        var segments = temp.split('/');
        var tableName = segments[1];
        var params = {
          column_set_id: _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].loggedInUserInfo['auths']['tables'][tableName]['edits'][0],
          in_form_column_name: this.name,
          single_column: this.name
        };
        var val = $('#' + this.name + '-' + this.record['id']).prop("checked");
        params[this.name] = val ? 1 : 0;
        var url = this.sessionHelper.getBackendUrlWithToken() + "tables/" + tableName + "/" + this.record['id'] + "/update";
        this.generalHelper.startLoading();
        this.sessionHelper.doHttpRequest("POST", url, params).then(data => {
          this.generalHelper.stopLoading();
          if (typeof data['message'] == "undefined") this.messageHelper.sweetAlert("Beklenmedik cevap geldi!", "Hata", "warning");else if (data['message'] == 'error') this.messageHelper.sweetAlert("Beklenmedik cevap geldi!", "Hata", "warning");else if (data['message'] == 'success') this.changed.emit(event);else this.messageHelper.sweetAlert("Beklenmedik cevap geldi!", "Hata", "warning");
        }).catch(e => {
          this.generalHelper.stopLoading();
        });
      }

