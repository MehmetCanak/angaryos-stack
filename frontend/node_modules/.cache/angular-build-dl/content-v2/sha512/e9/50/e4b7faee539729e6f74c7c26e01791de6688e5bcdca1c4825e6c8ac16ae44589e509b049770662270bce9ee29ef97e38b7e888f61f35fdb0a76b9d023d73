    var _helpers_data__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(
    /*! ./../helpers/data */
    "./src/aero/pages/helpers/data.ts");

    let ShowComponent = class ShowComponent {
      constructor(route, sessionHelper, generalHelper, aeroThemeHelper) {
        this.route = route;
        this.sessionHelper = sessionHelper;
        this.generalHelper = generalHelper;
        this.aeroThemeHelper = aeroThemeHelper;
        this.id = "";
        this.inShowTableName = "";
        this.showLoad = new _angular_core__WEBPACK_IMPORTED_MODULE_2__["EventEmitter"]();
        this.data = null;
        this.tableName = "";
        this.recordId = -1;
        this.defaultLimit = 3;
        this.editable = false;
        this.fillDefaultVariables();
        var th = this;
        setTimeout(() => route.params.subscribe(val => th.preLoadInterval(val)), 100);
      }

      ngOnChanges() {
        this.preLoadInterval();
      }

      preLoadInterval(val = null) {
        var params = {
          val: val,
          th: this
        };

        function func(params) {
          params.th.preLoad(params.val);
        }

        return _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].doInterval('formPreLoad', func, params, 200);
      }

      preLoad(val) {
        this.fillDefaultVariables();
        this.fillTableNameAndRecordId(val);
        this.addEventForFeatures();
        this.addEventForThemeIcons();
        this.dataReload();
        this.aeroThemeHelper.pageRutine();
      }

      fillDefaultVariables() {
        this.data = {};
        this.data['title'] = '';
        this.data['column_set'] = [];
        this.data['column_set']['column_set_type'] = 'none';
        this.data['column_set']['column_arrays'] = [];
      }

      fillEditable() {
        if (typeof _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].loggedInUserInfo['auths'] == "undefined") return;
        if (typeof _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].loggedInUserInfo['auths']['tables'] == "undefined") return;
        if (typeof _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].loggedInUserInfo['auths']['tables'][this.tableName] == "undefined") return;
        if (typeof _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].loggedInUserInfo['auths']['tables'][this.tableName]['edits'] == "undefined") return;
        this.editable = true;
      }

      fillTableNameAndRecordId(val) {
        if (this.inShowTableName == "") {
          this.tableName = val.tableName;
          this.recordId = val.recordId;
        } else {
          this.tableName = this.inShowTableName;
          this.recordId = parseInt(this.id);
        }
      }

      getParamsForShow() {
        var auths = _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].loggedInUserInfo['auths'];
        var params = {
          column_set_id: auths['tables'][this.tableName]['shows'][0]
        };
        return params;
      }

      dataReload() {
        var url = this.sessionHelper.getBackendUrlWithToken() + "tables/" + this.tableName + "/" + this.recordId;
        var params = this.getParamsForShow();
        var data = {
          'params': _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].objectToJsonStr(params)
        };
        this.sessionHelper.doHttpRequest("GET", url, data).then(data => this.dataLoaded(data));
      }

      dataLoaded(data) {
        this.data = this.fillDataAdditionalVariables(data);
        this.fillEditable();
        this.addEventForFeatures();
        this.showLoad.emit(data);
      }

      fillDataAdditionalVariables(data) {
        data['title'] = _helpers_data__WEBPACK_IMPORTED_MODULE_7__["DataHelper"].getTitleOrDefault(data['column_set']['name'], 'Bilgi Kartı');
        data['record']['json'] = _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].objectToJsonStr(data['record']);

        for (var i = 0; i < data['column_set']['column_arrays'].length; i++) {
          var json = _helpers_base__WEBPACK_IMPORTED_MODULE_4__["BaseHelper"].objectToJsonStr(data['column_set']['column_arrays'][i]);

          data['column_set']['column_arrays'][i]['json'] = json;

          var title = _helpers_data__WEBPACK_IMPORTED_MODULE_7__["DataHelper"].getTitleOrDefault(data['column_set']['column_arrays'][i]['name_basic'], '');

          data['column_set']['column_arrays'][i]['title'] = title;
        }

        return data;
      }

      edit() {
        this.generalHelper.navigate("table/" + this.tableName + "/" + this.recordId + "/edit");
      }
      /****    Events Functions    ****/


      addEventForFeatures() {
        this.aeroThemeHelper.addEventForFeature("standartElementEvents");
      }

      addEventForThemeIcons() {
        this.aeroThemeHelper.addEventForFeature("mobileMenuButton");
        this.aeroThemeHelper.addEventForFeature("rightIconToggleButton");
      }

    };

    ShowComponent.ctorParameters = () => [{
      type: _angular_router__WEBPACK_IMPORTED_MODULE_1__["ActivatedRoute"]
    }, {
      type: _helpers_session__WEBPACK_IMPORTED_MODULE_3__["SessionHelper"]
    }, {
      type: _helpers_general__WEBPACK_IMPORTED_MODULE_5__["GeneralHelper"]
    }, {
      type: _helpers_aero_theme__WEBPACK_IMPORTED_MODULE_6__["AeroThemeHelper"]
    }];

    tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_2__["Input"])()], ShowComponent.prototype, "id", void 0);
    tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_2__["Input"])()], ShowComponent.prototype, "inShowTableName", void 0);
    tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_2__["Output"])()], ShowComponent.prototype, "showLoad", void 0);
    ShowComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_2__["Component"])({
      selector: 'in-show-element',
      template: tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(
      /*! raw-loader!./show.component.html */
      "./node_modules/raw-loader/dist/cjs.js!./src/aero/pages/show/show.component.html")).default,
      styles: [tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(
      /*! ./show.component.scss */
      "./src/aero/pages/show/show.component.scss")).default]
    })], ShowComponent);
