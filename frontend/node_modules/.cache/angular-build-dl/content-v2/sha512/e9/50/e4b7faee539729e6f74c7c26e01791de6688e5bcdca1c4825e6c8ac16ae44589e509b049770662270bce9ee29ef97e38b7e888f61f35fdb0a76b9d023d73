    let PublicMapComponent = class PublicMapComponent {
      constructor(messageHelper, sessionHelper, aeroThemeHelper) {
        this.messageHelper = messageHelper;
        this.sessionHelper = sessionHelper;
        this.aeroThemeHelper = aeroThemeHelper;
        this.loggedInUserToken = "public";
        this.loggedInUserInfoJson = "";
        sessionHelper.doHttpRequest("GET", _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].backendUrl + "public/getLoggedInUserInfo").then(loggedInUserInfo => {
          this.loggedInUserInfoJson = _helpers_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].objectToJsonStr(loggedInUserInfo);
        }).catch(e => {
          this.messageHelper.toastMessage("Bir hata oluştu. Sonra tekrar deneyin");
