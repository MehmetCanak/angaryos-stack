        this.messageHelper = messageHelper;
        this.srid = "";
        this.upFormId = "";
        this.createForm = false;
        this.map = null;
        this.geoJsonObject = null;
        this.baseElementSelector = "";
        this.geoColumns = {
          'point': 'Nokta',
          'linestring': 'Çizgi',
          'polygon': 'Alan',
          'multipoint': 'Nokta(lar)',
          'multilinestring': 'Çizgi(ler)',
          'multipolygon': 'Alan(lar)'
        };
