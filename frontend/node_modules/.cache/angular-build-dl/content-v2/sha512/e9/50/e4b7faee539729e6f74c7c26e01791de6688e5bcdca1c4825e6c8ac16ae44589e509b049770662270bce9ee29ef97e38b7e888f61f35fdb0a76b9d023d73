        var rand = Math.floor(Math.random() * 10000) + 1;
        this.inFormElementId = "ife-" + rand;
        setTimeout(() => {
          $('#' + this.inFormElementId + 'inFormModal').modal('show');
        }, 100);
      }

      getTargetTableAndColumnForDataTransport(typeName) {
        return new Promise(resolve => {
          var loggedInUserId = _base__WEBPACK_IMPORTED_MODULE_5__["BaseHelper"].loggedInUserInfo['user']['id'];
          var key = 'user:' + loggedInUserId + '.dataTransport';

          var temp = _base__WEBPACK_IMPORTED_MODULE_5__["BaseHelper"].readFromLocal(key);

