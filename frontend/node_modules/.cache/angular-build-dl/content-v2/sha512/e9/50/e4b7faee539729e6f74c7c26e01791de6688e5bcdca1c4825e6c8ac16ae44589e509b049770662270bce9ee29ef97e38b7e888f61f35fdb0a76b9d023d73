        return this.tokenControl().then(data => {
          return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].loggedInUserInfo;
        });
      }

      toSeo(str) {
        str = str.replace(/ /g, "_");
        str = str.replace(/</g, "");
        str = str.replace(/>/g, "");
        str = str.replace(/"/g, "");
