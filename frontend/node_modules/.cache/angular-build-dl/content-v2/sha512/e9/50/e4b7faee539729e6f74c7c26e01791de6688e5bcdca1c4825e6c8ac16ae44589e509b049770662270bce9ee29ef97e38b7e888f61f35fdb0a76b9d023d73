    class BaseHelper {
      static preLoad() {
        this.fillUserData();
      }
      /****    General Function    ****/


      static sleep(ms) {
        return new Promise(resolve => setTimeout(resolve, ms));
      }

      static getObjectKeys(obj) {
        if (obj == null) return [];
        return Object.keys(obj);
      }

      static htmlStripTags(html) {
        return html.replace(/&/g, "&amp;").replace(/>/g, "&gt;").replace(/</g, "&lt;").replace(/"/g, "&quot;");
      }

      static replaceAll(str, oldStr, newStr) {
        return str.split(oldStr).join(newStr);
      }

      static doInterval(id, func, params, duration = 1000) {
        return new Promise(resolve => {
          id = "intervalId" + id;
          if (typeof this.pipe[id] != "undefined") clearInterval(this.pipe[id]);
          this.pipe[id] = setInterval(() => {
            clearInterval(this.pipe[id]);
            delete this.pipe[id];
            resolve(func(params));
          }, duration);
        });
      }

      static waitForOperationTest(resolve, func) {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
          var count = 20;
          var control = true;

          while (control) {
            try {
              func();
              control = false;
            } catch (error) {
              yield BaseHelper.sleep(100);
            }

            if (--count == 0) control = false;
          }

          resolve();
        });
      }

      static waitForOperation(func) {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
          return new Promise((resolve, error) => this.waitForOperationTest(resolve, func));
        });
      }

      static formatMoney(number, decPlaces, decSep, thouSep) {
        decPlaces = isNaN(decPlaces = Math.abs(decPlaces)) ? 2 : decPlaces, decSep = typeof decSep === "undefined" ? "." : decSep;
        thouSep = typeof thouSep === "undefined" ? "," : thouSep;
        var sign = number < 0 ? "-" : "";
        var i = String(parseInt(number = Math.abs(Number(number) || 0).toFixed(decPlaces)));
        var j = (j = i.length) > 3 ? j % 3 : 0;
        return sign + (j ? i.substr(0, j) + thouSep : "") + i.substr(j).replace(/(\decSep{3})(?=\decSep)/g, "$1" + thouSep) + (decPlaces ? decSep + Math.abs(number - parseInt(i)).toFixed(decPlaces).slice(2) : "");
      }
      /****    User Operation Functions    ****/


      static fillUserData() {
        this.fillTokenIfExist();
        this.fillLoggedInUserInfoIfExist();
      }

      static fillTokenIfExist() {
        var temp = this.readFromLocal("token");
        if (temp != null) this.token = temp;
      }

      static fillLoggedInUserInfoIfExist() {
        var temp = this.readFromLocal("loggedInUserInfo");
        if (temp != null) this.loggedInUserInfo = temp;
      }

      static setToken(token) {
        this.writeToLocal("token", token, this.tokenTimeOut);
        this.token = token;
      }

      static setLoggedInUserInfo(info) {
        this.writeToLocal("loggedInUserInfo", info, this.tokenTimeOut);
        this.loggedInUserInfo = info;
      }

      static clearUserData() {
        this.clearToken();
        this.clearLoggedInUserInfo();
      }

      static clearToken() {
        this.removeFromLocal("token");
        this.token = "";
      }

      static clearLoggedInUserInfo() {
        this.removeFromLocal("loggedInUserInfo");
