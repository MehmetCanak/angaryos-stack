        this.changed = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
        this.val = null;
      }

      ngAfterViewInit() {}

      ngOnChanges() {
        if (this.valueJson == "") return;
        this.val = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].jsonStrToObject(this.valueJson);
        if (typeof this.val == "string") this.val = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].jsonStrToObject(this.val);
      }

