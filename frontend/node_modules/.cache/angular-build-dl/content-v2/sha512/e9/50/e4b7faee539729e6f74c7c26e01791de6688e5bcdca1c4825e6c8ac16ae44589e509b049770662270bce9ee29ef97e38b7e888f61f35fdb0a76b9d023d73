  function srcAeroPagesHelpersCustomElementsRelationColumnElementRelationColumnElementComponentTs(module, __webpack_exports__, __webpack_require__) {
    "use strict";

    __webpack_require__.r(__webpack_exports__);
    /* harmony export (binding) */


    __webpack_require__.d(__webpack_exports__, "RelationColumnElementComponent", function () {
      return RelationColumnElementComponent;
    });
    /* harmony import */


    var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(
    /*! tslib */
    "./node_modules/tslib/tslib.es6.js");
    /* harmony import */


    var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(
    /*! @angular/core */
    "./node_modules/@angular/core/fesm2015/core.js");
    /* harmony import */


    var _base__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(
    /*! ./../../base */
    "./src/aero/pages/helpers/base.ts");
    /* harmony import */


    var _general__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(
    /*! ./../../general */
    "./src/aero/pages/helpers/general.ts");
    /* harmony import */


    var _session__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(
    /*! ./../../session */
    "./src/aero/pages/helpers/session.ts");
    /* harmony import */


    var _message__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(
    /*! ./../../message */
    "./src/aero/pages/helpers/message.ts");

    let RelationColumnElementComponent = class RelationColumnElementComponent {
      constructor(generalHelper, sessionHelper, messageHelper) {
        this.generalHelper = generalHelper;
        this.sessionHelper = sessionHelper;
        this.messageHelper = messageHelper;
        this.type = "";
        this.name = "";
        this.relationJson = "";
        this.recordJson = "";
        this.tableName = "";
        this.typeBase = "";
        this.record = null;
        this.relation = null;
        this.items = [];
      }

      ngAfterViewInit() {}

      ngOnChanges() {
        this.typeBase = this.type.split(':')[0];

        if (this.recordJson != "") {
          this.record = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].jsonStrToObject(this.recordJson);
          this.fillMultiItems();
        }

        if (this.relationJson != "") this.relation = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].jsonStrToObject(this.relationJson);
      }

      fillMultiItems() {
        if (this.typeBase == 'select') return;

        if (this.record[this.name] == null) {
          this.items = [];
          return;
        }

        if (this.record[this.name] == "") {
          this.items = [];
          return;
        }

        var temp = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].jsonStrToObject(this.record[this.name]);

        if (typeof temp != "object") {
          this.items = temp;
          return;
        }

        var arr = [];
        var keys = Object.keys(temp);

        for (var i = 0; i < keys.length; i++) arr.push(temp[keys[i]]);

        this.items = arr;
      }

      getRelationDataUrl() {
        if (this.relation == null) return "";

        if (this.tableName.length == 0) {
          var temp = window.location.href;
          temp = temp.replace(_base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].baseUrl, "");
          var segments = temp.split('/');
          this.tableName = segments[1];
        }

        var url = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].backendUrl + _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].token;
        url += "/tables/" + this.tableName + "/" + this.record['id'] + "/getRelationDataInfo/" + this.name;
        return url;
      }

      openRelationDataInfoPage(event, item = null) {
        if (event.ctrlKey == false && event.altKey == false) return;
        var url = this.getRelationDataUrl();
        if (item != null) url += "?source=" + item['source'];
        this.generalHelper.startLoading();
        this.sessionHelper.doHttpRequest("GET", url).then(data => {
          this.generalHelper.stopLoading();

          if (typeof data['recordId'] == "undefined") {
            this.messageHelper.toastMessage("Bu kaydın bilgi kartı gösterilemez");
            return;
          }

          if (!this.authControlForRelationDataInfoPage(data)) {
            this.messageHelper.toastMessage("Bilgi kartı için yetkiniz yok");
            return;
          }

          this.generalHelper.navigate('table/' + data['tableName'] + "/" + data['recordId'], event.ctrlKey);
        }).catch(e => {
          this.generalHelper.stopLoading();
        });
      }

      authControlForRelationDataInfoPage(data) {
        var auth = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].loggedInUserInfo['auths']['tables'];
        if (typeof auth[data['tableName']] == "undefined") return false;
        if (typeof auth[data['tableName']]['shows'] == "undefined") return false;
        return true;
      }

    };

    RelationColumnElementComponent.ctorParameters = () => [{
      type: _general__WEBPACK_IMPORTED_MODULE_3__["GeneralHelper"]
    }, {
      type: _session__WEBPACK_IMPORTED_MODULE_4__["SessionHelper"]
    }, {
      type: _message__WEBPACK_IMPORTED_MODULE_5__["MessageHelper"]
    }];

