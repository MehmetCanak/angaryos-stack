        this.dataChanged = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
      }
      /****    Map Functions    ****/


      ngAfterViewInit() {
        if (this.upFormId.length > 0) this.baseElementSelector = '[ng-reflect-id="' + this.upFormId + '"] ';
      }

      openMapElement() {
        this.showModal().then(() => this.createMapElement()).then(map => this.addDrawing(map)).then(map => this.addFeaturesFromValue(map)).then(map => this.emitDataChangedEvent());
      }

      showModal() {
        return new Promise(resolve => {
          $(this.baseElementSelector + ' #' + this.name + 'ElementModal').modal('show').on('shown.bs.modal', () => {
            resolve(true);
          });
        });
      }

      createMapElement() {
        return _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].createFormElementMap(this.name + 'MapElement').then(map => {
          this.map = map;
          return map;
        });
      }

      addDrawing(map) {
        var type = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].ucfirst(this.type.replace("multi", ""));

        if (type == "Linestring") type = "LineString";

        _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].addDraw(map, type, this.multiple);

        _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].addModify(map, this.multiple);

        _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].addSnap(map);

        map.addEventListener('dataChanged', e => this.emitDataChangedEvent(), true);
        return map;
      }

      addFeaturesFromValue(map) {
        if (this.value == "") return map;
        if (this.value.substr(0, 2) != '["') this.value = '["' + this.value + '"]';
        var srid = null;
        if (this.srid.length > 0) srid = "EPSG:" + this.srid;

        var wkts = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].jsonStrToObject(this.value);

        for (var i = 0; i < wkts.length; i++) _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].addFeatureByWkt(map, wkts[i], srid);

        return map;
      }

      selectFeature(i) {
        _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].selectFeature(this.map, i);
      }

      deleteFeature(i) {
        _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].deleteFeature(this.map, i);

        this.emitDataChangedEvent();
      }

      addNetcadFeatures() {
        setTimeout(() => $(document).off('focusin.modal'), 500);
        this.messageHelper.swalPrompt("Netcad nokta dizisi:", "Tamam", "İptal", "textarea").then(data => {
          if (typeof data["value"] == "undefined") return;

          var wkt = _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].getWktFromNetcad(data['value'], this.type.toLowerCase().replace('multi', ''));

          var feature = _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].getFeatureFromWkt(wkt, _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].userProjection);

          _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].addFeatures(this.map, [feature]);

          _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].zoomToFeature(this.map, feature);

          if (!this.multiple) {
            _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].showNoMultipleConfirm(this.map).then(() => this.emitDataChangedEvent());
          }
        });
      }
      /****    Gui Functions    ****/


      getDescription() {
        if (this.value == null) return this.placeholder;
        if (this.value.length == 0) return this.placeholder;
        return this.geoColumns[this.type];
      }

      getStyle() {
        if (this.value == null) return {
          color: '#999'
        };
        if (this.value.length == 0) return {
          color: '#999'
        };
        return {
          color: '#495057'
        };
      }

      getTransformedCoords(coords) {
        return _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].transformCoorditanes(coords);
      }
      /****    Event Functions    ****/


      emitDataChangedEvent() {
        this.geoJsonObject = _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].getAllFeaturesAsGeoJsonObject(this.map).features;
        this.dataChanged.emit(this.geoJsonObject);
      }

      emitChangedEvent() {
        var srid = null;
        if (this.srid.length > 0) srid = "EPSG:" + this.srid;

        var wkt = _map__WEBPACK_IMPORTED_MODULE_3__["MapHelper"].getAllFeaturesAsWkt(this.map, srid);

        var json = _base__WEBPACK_IMPORTED_MODULE_2__["BaseHelper"].objectToJsonStr(wkt);

        this.value = json;
        $(this.baseElementSelector + ' #' + this.name).val(json);
        const changeEvent = document.createEvent('Event');
        changeEvent.initEvent('change', true, true);
        $(this.baseElementSelector + ' #' + this.name)[0].dispatchEvent(changeEvent);
        $(this.baseElementSelector + ' #' + this.name + 'ElementModal').modal('hide');
      }

      handleChange(event) {
        this.changed.emit(event);
      }

    };

