      static convertDataByGuiTypeJsonb(guiType, data) {
        if (typeof data == "string") return data;
        return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].objectToJsonStr(data);
      }

      static convertDataByGuiTypeBoolean(record, columnName, guiType, data) {
        var id = 0;
        if (record != null) id = record['id'];

        switch (guiType) {
          case 'boolean':
          case 'boolean:fastchange':
            return data ? 'Aktif' : 'Pasif';

          default:
            return data.toString();
        }
      }

      static convertDataByGuiTypeFiles(guiType, data) {
        if (data == null) return null;
        var rt = "";
        var temp = data;
        if (typeof temp == "string") temp = _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].jsonStrToObject(data);

        for (var i = 0; i < temp.length; i++) {
          rt += "<a data-fancybox='gallery' href='" + _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].getFileUrl(temp[i], 'b_') + "'>"; //rt += "<a href='"+BaseHelper.getFileUrl(temp[i], 'b_')+"'>"

          rt += "<img src='" + _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].getFileUrl(temp[i], 's_') + "' class='rounded-circle file-preview' alt=''>";
          rt += "</a>";
        }

        return rt;
      }

      static convertDataByGuiTypeText(guiType, data, show) {
        if (data == null) return null;
        if (show) return "<pre>" + data + "</pre>";
        return data.substr(0, 100) + (data.length > 100 ? '...' : '');
      }

      static convertDataByGuiTypeMoney(guiType, data) {
        var unit = guiType.split(':')[1].toUpperCase();
        data = _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].formatMoney(data, 2, '.', ','); //data.replace(/(\d)(?=(\d{3})+\.)/g, '$1,');  

        return data + " " + unit;
      }

      static convertDataByGuiTypeDateTime(guiType, data) {
        switch (guiType) {
          case 'datetime':
            return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].dBDateTimeStringToHumanDateTimeString(data);

          case 'date':
            return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].dBDateStringToHumanDateString(data);

          default:
            return data;
        }
      }
      /****    Clear Data Functions */


      static changeDataForFormByGuiType(guiType, data) {
        switch (guiType.split(':')[0]) {
          case 'boolean':
            data = this.changeDataForFormByGuiTypeBoolean(data);
            break;

          case 'multiselect':
          case 'multiselectdragdrop':
            data = this.changeDataForFormByGuiTypeMultiSelect(data);
            break;

          case 'datetime':
            data = _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].humanDateTimeStringToDBDateTimeString(data);
            break;

          case 'date':
            data = _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].humanDateStringToDBDateString(data);
            break;

          case 'point':
          case 'linestring':
          case 'polygon':
          case 'multipoint':
          case 'multilinestring':
          case 'multipolygon':
            data = this.changeDataForFormByGuiTypeGeo(guiType, data);
            if (data.length > 0 && guiType.indexOf("multi") > -1) if (data.indexOf("MULTI") < 0) {
              data = guiType.toUpperCase() + "(" + data + ")";
            }
            break;
        }

        return data;
      }

      static changeDataForFormByGuiTypeGeo(guiType, data) {
        if (data.indexOf('["') < 0) return data;

        var obj = _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].jsonStrToObject(data);

        if (obj == null) return null;
        if (guiType.indexOf("multi") < 0) return obj[0];else {
          var wkt = guiType.toUpperCase() + "(";
          if (guiType == "multipolygon") wkt += "(";
          var temp = data.split('(');

          for (var i = 0; i < temp.length; i++) if (temp[i].indexOf(')') > -1) {
            var coords = temp[i].split(')')[0];
            wkt += "(" + coords + "),";
          }

          wkt = wkt.substr(0, wkt.length - 1) + ")";
          if (guiType == "multipolygon") wkt += ")";
          return wkt;
        }
      }

      static changeDataForFormByGuiTypeMultiSelect(data) {
        if (typeof data == "undefined") return "";
        if (data.length == 1 && data[0] == "") return "[]";
        return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].objectToJsonStr(data);
      }

      static changeDataForFormByGuiTypeBoolean(data) {
        if (typeof data == "undefined") return "";

        switch (data) {
          case null:
          case "":
            return "";

          case "true":
            return "1";

          case "false":
            return "0";

          default:
            alert("changeDataForFormByGuiTypeBoolean type error");
            return data;
            break;
        }
      }

      static changeDataForFilterByGuiType(guiType, data, elementName, columnName, localKey) {
        switch (guiType) {
          case 'boolean':
            data = this.changeDataForFilterByGuiTypeBoolean(data);
            break;

          case 'date':
            data = this.changeDataForFilterByGuiTypeDate(data);
            break;

          case 'datetime':
            data = this.changeDataForFilterByGuiTypeDateTime(data);
            break;

          case 'select':
          case 'multiselect':
            data = this.changeDataForFilterByGuiTypeSelectAndMultiSelect(columnName, elementName, localKey);
            break;
        }

        return data;
      }

      static changeDataForFilterByGuiTypeBoolean(data) {
        return data == "true";
      }

      static changeDataForFilterByGuiTypeDateTime(data) {
        data = data.replace('_', '');
        if (data.length < 19) return "";
        return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].humanDateTimeStringToDBDateTimeString(data);
      }

      static changeDataForFilterByGuiTypeDate(data) {
        if (data == null) return "";
        data = data.replace('_', '');
        if (data.length < 10) return "";
        return _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].humanDateStringToDBDateString(data);
      }

      static changeDataForFilterByGuiTypeSelectAndMultiSelect(columnName, elementName, localKey) {
        var data = [];
        var temp = $('[name="' + elementName + '"]').val();

        for (var i = 0; i < temp.length; i++) {
          if (temp[i] == null || temp[i] == "") {
            $('[name="' + elementName + '"] option[value=""]').remove();
            continue;
          }

          var text = $("[name='" + elementName + "'] option[value='" + temp[i] + "']").html().trim();
          var key = localKey + ".selectQueryElementDataCache." + columnName + "." + temp[i];

          _base__WEBPACK_IMPORTED_MODULE_1__["BaseHelper"].writeToLocal(key, text);

          data.push(temp[i]);
        }

        return data;
      }

    }
