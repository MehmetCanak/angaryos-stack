    "./src/aero/pages/helpers/data.ts");

    let BooleanElementComponent = class BooleanElementComponent {
      constructor() {
        this.createForm = false;
        this.displayNameForTrue = "true";
        this.displayNameForFalse = "false";
        this.changed = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
        setTimeout(() => {
          this.displayNameForTrue = _data__WEBPACK_IMPORTED_MODULE_2__["DataHelper"].convertDataByGuiTypeBoolean(null, null, this.guiType, true);
          this.displayNameForFalse = _data__WEBPACK_IMPORTED_MODULE_2__["DataHelper"].convertDataByGuiTypeBoolean(null, null, this.guiType, false);
          if (this.value.length > 0) return;
          if (this.createForm != true) return;
          if (this.defaultData.length == 0) return;
          this.setDefaultData();
        }, 50);
      }

      handleChange(event) {
        this.changed.emit(event);
      }

      getDisplayName(data) {
        if (data) return this.displayNameForTrue;else return this.displayNameForFalse;
      }

      setDefaultData() {
        if (this.defaultData === "true") this.value = "true";else if (this.defaultData === "false") this.value = "false";
      }

    };
